  },
  "homepage": "https://github.com/mdm373/ny-data-web#readme",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "css-loader": "^3.3.0",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
