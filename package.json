{
  "name": "ny-data-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsc -w",
    "clean": "sh ./scripts/clean.sh",
    "wait-for-serve": "wait-on ./.temp/tsc/server/index.js",
    "serve": "npm run wait-for-serve && nodemon ./.temp/tsc/server --watch ./.temp/tsc/server",
    "copy:normalize": "mkdir -p ./.temp/pack",
    "copy:index": "cp index.html ./.temp/pack/index.html",
    "copy:bootstrap": "cp ./node_modules/bootstrap/dist/js/bootstrap.min.js ./.temp/pack/bootstrap.min.js",
    "copy:jquery": "cp ./node_modules/jquery/dist/jquery.slim.min.js ./.temp/pack/jquery.slim.min.js",
    "copy:popper": "cp ./node_modules/popper.js/dist/umd/popper.min.js ./.temp/pack/popper.min.js",
    "copy:fav-icon": "cp ./favicon.ico ./.temp/pack/favicon.ico",
    "copy": "run-s copy:*",
    "swagger-gen:local" : "sh ./scripts/swagger-gen.sh .APP_CONFIG.LOCAL.apiDomain",
    "swagger-gen:hosted" : "sh ./scripts/swagger-gen.sh .APP_CONFIG.HOSTED.apiDomain",
    "pack-dev": "npm run copy && APP_CONFIG_ENV=local webpack --watch",
    "serve-dev": "npm run clean && npm run swagger-gen:local && run-p -r dev pack-dev serve",
    "deploy": "npm run swagger-gen:hosted && sh ./scripts/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdm373/ny-data-web.git"
  },
  "author": "mdm373@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdm373/ny-data-web/issues"
  },
  "homepage": "https://github.com/mdm373/ny-data-web#readme",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/mapbox__polyline": "^1.0.0",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.3.0",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.1",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0",
    "portable-fetch": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  }
}
