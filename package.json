{
  "name": "ny-data-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsc -w",
    "clean": "sh ./scripts/clean.sh",
    "wait-for-serve": "wait-on ./.temp/tsc/local/server.js",
    "serve": "npm run wait-for-serve && sh ./scripts/build.sh && nodemon --watch ./.temp/tsc ./.temp/tsc/local/server",
    "copy:index": "mkdir -p ./.temp/pack && cp index.html ./.temp/pack/index.html",
    "pack-dev": "npm run copy:index && webpack --watch",
    "serve-dev": "npm run clean && run-p -r dev pack-dev serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdm373/ny-data-web.git"
  },
